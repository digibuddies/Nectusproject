<?xml version="1.0" encoding="utf-8"?>
<agency.tango.materialintroscreen.parallax.ParallaxLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/rl">

        <com.whygraphics.gifview.gif.GIFView
            android:id="@+id/main_activity_gif_vie2"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            android:layout_height="170dp" />

        <ImageView
            android:layout_width="match_parent"
            app:srcCompat="@drawable/back"
            android:id="@+id/imageView5"
            android:scaleType="fitXY"
            android:layout_height="170dp" />

        <ImageView
            android:layout_width="wrap_content"
            app:srcCompat="@drawable/dfd"
            android:id="@+id/imageView3"
            android:layout_marginTop="25dp"
            android:layout_height="150dp" />

        <CheckBox
            android:id="@+id/checkBox89"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="32dp"
            android:text="pass"
            app:layout_parallaxFactor="0.4"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="13dp"
            android:layout_marginTop="16dp"
            android:visibility="invisible" />


        <TextView
            android:text="Contact Details"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/kdate"
            android:textSize="30sp"
            android:textColor="@android:color/primary_text_dark"
            android:fontFamily="cursive"
            android:layout_centerVertical="false"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/avatar3"
            android:layout_marginTop="10dp"
            android:textStyle="normal|bold" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/avatar3"
            android:src="@mipmap/ic_launcher"
            app:civ_border_width="2dp"
            app:civ_border_color="@android:color/white"
            android:layout_gravity="top|center_horizontal"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginTop="77dp"
            android:layout_below="@+id/checkBox89"
            android:layout_centerHorizontal="true" />

        <RelativeLayout
            android:layout_height="200dp"
            android:layout_width="match_parent"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="20dp"
            android:layout_below="@+id/kdate"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            android:focusable="true"
            android:focusableInTouchMode="true">


            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="Enter Email Address"
                android:id="@+id/editText"
                android:textColor="@android:color/white"
                android:textColorHint="@android:color/black"
                android:layout_below="@+id/tView2"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:fontFamily="serif"
                android:inputType="textEmailAddress"
                android:cursorVisible="true"
                android:textColorHighlight="@android:color/black"
                android:textColorLink="@android:color/white"
                android:shadowColor="@android:color/white"
                android:imeOptions="actionNext" />

            <TextView
                android:text="I would not want to connect if :"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView10"
                android:textSize="18sp"
                android:textColor="@android:color/white"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/Spinnerr1"
                android:layout_centerHorizontal="true"
                android:fontFamily="serif" />

            <Spinner
                android:id="@+id/Spinnerr1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:entries="@array/listt1"
                android:prompt="@array/listt1"
                android:textSize="20sp"
                android:layout_below="@+id/textView5"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/tView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Contact me On :"
                android:textColor="@android:color/white"
                android:textSize="20sp"
                android:fontFamily="serif"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="0dp" />

            <ImageView
                android:layout_height="2dp"
                app:srcCompat="@android:color/white"
                android:id="@+id/imageView"
                android:layout_below="@+id/editText"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="15dp"
                android:layout_width="300dp"
                android:focusable="false"
                android:focusableInTouchMode="false"/>

            <TextView
                android:text="I would like to connect if :"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView5"
                android:textSize="18sp"
                android:textColor="@android:color/white"
                android:layout_marginTop="18dp"
                android:fontFamily="serif"
                android:layout_below="@+id/imageView"
                android:layout_centerHorizontal="true"
                android:focusable="true"
                android:focusableInTouchMode="true" />

            <Spinner
                android:id="@+id/Spinnerr2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:entries="@array/listt2"
                android:prompt="@array/listt2"
                android:textSize="20sp"
                android:layout_marginTop="13dp"
                android:layout_below="@+id/textView10"
                android:layout_centerHorizontal="true"
                android:textAlignment="center" />

        </RelativeLayout>

    </RelativeLayout>

</agency.tango.materialintroscreen.parallax.ParallaxLinearLayout>

