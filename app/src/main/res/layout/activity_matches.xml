<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:arc="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.digibuddies.nectus.matches"
    android:id="@+id/container"
    android:background="@drawable/proback">

    <com.bvapp.arcmenulibrary.widget.ObservableScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="130dp"
        android:id="@+id/sv"
        android:visibility="invisible">

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="300px">

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_alignParentStart="false"
                android:layout_alignParentBottom="false"
                android:clickable="true"
                android:contextClickable="true"
                app:cardBackgroundColor="@android:color/white"
                android:id="@+id/cd3"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="0dp"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="match_parent"
                        app:srcCompat="@drawable/color2"
                        android:id="@+id/imageView"
                        android:layout_height="70dp" />

                    <Button
                        android:text="Click to See Contact Details"
                        android:layout_height="wrap_content"
                        android:id="@+id/b1"
                        android:layout_width="100dp"
                        android:layout_gravity="right"
                        android:textColor="@android:color/white"
                        android:background="@android:color/holo_blue_light"
                        android:textSize="10sp"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true" />

                    <TextView
                        android:text="user"
                        android:id="@+id/userc"
                        android:layout_width="200dp"
                        android:layout_height="30dp"
                        android:textSize="22sp"
                        android:layout_gravity="center_horizontal"
                        android:textAlignment="center"
                        android:textStyle="bold|italic"
                        android:textColor="@android:color/black"
                        android:fontFamily="sans-serif-smallcaps"
                        android:layout_below="@+id/avc"
                        android:layout_centerHorizontal="true" />

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/avc"
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:src="@drawable/s1"
                        app:civ_border_width="2dp"
                        app:civ_border_color="@android:color/white"
                        android:layout_marginTop="13dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" />

                    <TextView
                        android:text="TextView"
                        android:layout_width="match_parent"
                        android:id="@+id/detailc"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        android:textColor="@android:color/black"
                        android:textSize="16sp"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/userc"
                        android:layout_centerHorizontal="true" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/q"
                        android:id="@+id/imageView8"
                        android:layout_gravity="bottom"
                        android:layout_below="@+id/detailc"
                        android:layout_alignParentStart="true" />

                    <TextView
                        android:text="Share Contact Detail"
                        android:layout_width="wrap_content"
                        android:id="@+id/textView2"
                        android:textSize="14sp"
                        android:textColor="@android:color/black"
                        android:textStyle="normal|italic"
                        android:fontFamily="serif"
                        android:layout_marginRight="5dp"
                        android:layout_gravity="bottom|right"
                        android:layout_alignEnd="@+id/detailc"
                        android:layout_below="@+id/imageView8"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:text="match"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:id="@+id/tv11"
                        android:layout_gravity="bottom|left"
                        android:textStyle="normal|italic"
                        android:textSize="16sp"
                        android:layout_marginLeft="5dp"
                        android:textColor="@android:color/black"
                        android:fontFamily="serif"
                        android:layout_below="@+id/imageView8"
                        android:layout_alignStart="@+id/detailc"
                        android:layout_marginStart="5dp" />

                    <io.ghyeok.stickyswitch.widget.StickySwitch
                        android:id="@+id/ss"
                        android:layout_width="wrap_content"
                        android:layout_height="38dp"
                        android:layout_centerInParent="false"
                        app:iconPadding="10dp"
                        app:iconSize="16dp"
                        app:leftIcon="@drawable/no"
                        app:rightIcon="@drawable/ic_check_black_24dp"
                        app:sliderBackgroundColor="@color/colorSliderBackground"
                        app:switchColor="@color/colorSwitchColor"
                        app:textColor="@android:color/black"
                        android:layout_alignParentBottom="true"
                        android:layout_alignEnd="@+id/textView2"
                        android:layout_below="@+id/textView2"
                        android:layout_marginBottom="2dp" />
                </RelativeLayout>


            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_alignParentStart="false"
                android:layout_alignParentEnd="false"
                android:layout_alignParentLeft="false"
                android:layout_alignParentTop="false"
                android:layout_centerHorizontal="false"
                android:layout_centerInParent="false"
                android:layout_centerVertical="false"
                android:layout_marginLeft="20dp"
                android:clickable="true"
                app:cardBackgroundColor="@android:color/white"
                android:layout_alignParentBottom="false"
                android:id="@+id/cd2"
                android:layout_marginTop="20dp"
                android:layout_marginRight="20dp"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="match_parent"
                        app:srcCompat="@drawable/color2"
                        android:id="@+id/imageView2"
                        android:layout_height="70dp" />

                    <Button
                        android:text="Click to See Contact Details"
                        android:layout_height="wrap_content"
                        android:id="@+id/b2"
                        android:layout_width="100dp"
                        android:layout_gravity="right"
                        android:textColor="@android:color/white"
                        android:background="@android:color/holo_blue_light"
                        android:textSize="10sp"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true" />

                    <TextView
                        android:text="user"
                        android:id="@+id/tv2"
                        android:layout_width="200dp"
                        android:layout_height="30dp"
                        android:textSize="22sp"
                        android:layout_gravity="center_horizontal"
                        android:textAlignment="center"
                        android:textStyle="bold|italic"
                        android:textColor="@android:color/black"
                        android:fontFamily="sans-serif-smallcaps"
                        android:layout_below="@+id/p2"
                        android:layout_centerHorizontal="true" />

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/p2"
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:src="@drawable/s1"
                        app:civ_border_width="2dp"
                        app:civ_border_color="@android:color/white"
                        android:layout_marginTop="13dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" />

                    <TextView
                        android:text="TextView"
                        android:layout_width="match_parent"
                        android:id="@+id/detail2"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        android:textColor="@android:color/black"
                        android:textSize="16sp"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv2"
                        android:layout_centerHorizontal="true" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/q"
                        android:id="@+id/imageView22"
                        android:layout_gravity="bottom"
                        android:layout_below="@+id/detail2"
                        android:layout_alignParentStart="true" />

                    <TextView
                        android:text="Share Contact Detail"
                        android:layout_width="wrap_content"
                        android:id="@+id/textView22"
                        android:textSize="14sp"
                        android:textColor="@android:color/black"
                        android:textStyle="normal|italic"
                        android:fontFamily="serif"
                        android:layout_marginRight="5dp"
                        android:layout_gravity="bottom|right"
                        android:layout_alignEnd="@+id/detail2"
                        android:layout_below="@+id/imageView22"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:text="match"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:id="@+id/tv22"
                        android:layout_gravity="bottom|left"
                        android:textStyle="normal|italic"
                        android:textSize="16sp"
                        android:layout_marginLeft="5dp"
                        android:textColor="@android:color/black"
                        android:fontFamily="serif"
                        android:layout_below="@+id/imageView22"
                        android:layout_alignStart="@+id/detail2"
                        android:layout_marginStart="5dp" />

                    <io.ghyeok.stickyswitch.widget.StickySwitch
                        android:id="@+id/ss2"
                        android:layout_width="wrap_content"
                        android:layout_height="38dp"
                        android:layout_centerInParent="false"
                        app:iconPadding="10dp"
                        app:iconSize="16dp"
                        app:leftIcon="@drawable/no"
                        app:rightIcon="@drawable/ic_check_black_24dp"
                        app:sliderBackgroundColor="@color/colorSliderBackground"
                        app:switchColor="@color/colorSwitchColor"
                        app:textColor="@android:color/black"
                        android:layout_alignParentBottom="true"
                        android:layout_alignEnd="@+id/textView22"
                        android:layout_below="@+id/textView22"
                        android:layout_marginBottom="2dp" />
                </RelativeLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:layout_alignParentStart="true"
                android:layout_marginTop="20dp"
                android:layout_width="match_parent"
                android:layout_marginLeft="20dp"
                android:clickable="true"
                app:cardBackgroundColor="@android:color/white"
                android:id="@+id/cd1"
                android:layout_marginRight="20dp"
                android:layout_marginBottom="10dp"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:layout_width="match_parent"
                        app:srcCompat="@drawable/color2"
                        android:id="@+id/imageView3"
                        android:layout_height="70dp" />

                    <Button
                        android:text="Click to See Contact Details"
                        android:layout_height="wrap_content"
                        android:id="@+id/b3"
                        android:layout_width="100dp"
                        android:layout_gravity="right"
                        android:textColor="@android:color/white"
                        android:background="@android:color/holo_blue_light"
                        android:textSize="10sp"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true" />

                    <TextView
                        android:text="user"
                        android:id="@+id/tv3"
                        android:layout_width="200dp"
                        android:layout_height="30dp"
                        android:textSize="22sp"
                        android:layout_gravity="center_horizontal"
                        android:textAlignment="center"
                        android:textStyle="bold|italic"
                        android:textColor="@android:color/black"
                        android:fontFamily="sans-serif-smallcaps"
                        android:layout_below="@+id/p3"
                        android:layout_centerHorizontal="true" />

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/p3"
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:src="@drawable/s1"
                        app:civ_border_width="2dp"
                        app:civ_border_color="@android:color/white"
                        android:layout_marginTop="13dp"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true" />

                    <TextView
                        android:text="TextView"
                        android:layout_width="match_parent"
                        android:id="@+id/detail3"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        android:textColor="@android:color/black"
                        android:textSize="16sp"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv3"
                        android:layout_centerHorizontal="true" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/q"
                        android:id="@+id/imageView33"
                        android:layout_gravity="bottom"
                        android:layout_below="@+id/detail3"
                        android:layout_alignParentStart="true" />

                    <TextView
                        android:text="Share Contact Detail"
                        android:layout_width="wrap_content"
                        android:id="@+id/textView3"
                        android:textSize="14sp"
                        android:textColor="@android:color/black"
                        android:textStyle="normal|italic"
                        android:fontFamily="serif"
                        android:layout_marginRight="5dp"
                        android:layout_gravity="bottom|right"
                        android:layout_alignEnd="@+id/detail3"
                        android:layout_below="@+id/imageView33"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:text="match"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:id="@+id/tv33"
                        android:layout_gravity="bottom|left"
                        android:textStyle="normal|italic"
                        android:textSize="16sp"
                        android:layout_marginLeft="5dp"
                        android:textColor="@android:color/black"
                        android:fontFamily="serif"
                        android:layout_below="@+id/imageView33"
                        android:layout_alignStart="@+id/detail3"
                        android:layout_marginStart="5dp" />

                    <io.ghyeok.stickyswitch.widget.StickySwitch
                        android:id="@+id/ss3"
                        android:layout_width="wrap_content"
                        android:layout_height="38dp"
                        android:layout_centerInParent="false"
                        app:iconPadding="10dp"
                        app:iconSize="16dp"
                        app:leftIcon="@drawable/no"
                        app:rightIcon="@drawable/ic_check_black_24dp"
                        app:sliderBackgroundColor="@color/colorSliderBackground"
                        app:switchColor="@color/colorSwitchColor"
                        app:textColor="@android:color/black"
                        android:layout_alignParentBottom="true"
                        android:layout_alignEnd="@+id/textView3"
                        android:layout_below="@+id/textView3"
                        android:layout_marginBottom="2dp" />
                </RelativeLayout>
            </android.support.v7.widget.CardView>
        </LinearLayout>

</com.bvapp.arcmenulibrary.widget.ObservableScrollView>

    <com.bvapp.arcmenulibrary.ArcMenu
        android:id="@+id/arcMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        arc:menuType="normal"
        arc:menuChildSize="48dp"
        arc:menuGravity="Bottom_Right"
        arc:menuClickAnim="true"
        arc:menuChildAnim="false"
        arc:menuShadowElevation="true"
        arc:menuNormalColor="@android:color/white"
        arc:menuImage="@drawable/ic_fingerprint_black_24dp">

        <TextView
            android:text="Not enough data to process! Please answer some more questions..."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/front"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textAlignment="center"
            android:textSize="24sp"
            android:fontFamily="sans-serif-smallcaps" />
    </com.bvapp.arcmenulibrary.ArcMenu>

    <ImageView
        android:layout_width="wrap_content"
        app:srcCompat="@drawable/matches"
        android:id="@+id/imageView4"
        android:layout_alignParentTop="false"
        android:layout_alignParentStart="false"
        android:layout_height="162dp" />

</RelativeLayout>
